USEMODULE_INCLUDES_pami := $(LAST_MAKEFILEDIR)/include
USEMODULE_INCLUDES += $(USEMODULE_INCLUDES_pami)

PROTOBUF_PATH_pami := $(LAST_MAKEFILEDIR)
PROTOBUF_PATH += $(PROTOBUF_PATH_pami)

ifdef PCA9685_OFFSET
	CFLAGS += -DPCA9685_OFFSET=$(PCA9685_OFFSET)
endif

ifeq ($(CPU), native64)
	CFLAGS += -DUART_NUMOF=3
endif
